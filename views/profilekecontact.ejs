<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <link rel='stylesheet' href='style.css' />
    <title> Association 1 </title>
  </head>
  <body>

    <h1> SHOW  Data Contact dari Profile <%= panggilData[0].first_name %> adalah =</h1>
<br>
<br>
    <table>
      <thead>
        <tr>
              <th> Contact Id </th>
              <th> first_name dr contact</th>
                <th> last_name dr contact</th>
          </tr>
        </thead>

      <tbody>
        <% panggilData.forEach(function(profile) { %>
      <tr>
      <th> <%= profile.ContactId %> </th>
      <th> <%= profile.first_name %> </th>
      <th> <%= profile.last_name %> </th>
      </tr>
          <%  }) %>
      </tbody>
    <table>
<br>
<br>
<br>

        </body>
      </html>
